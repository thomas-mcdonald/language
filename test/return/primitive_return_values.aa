#-#-#
class Main
  def add(int x, int y) : int
    x + y
  end

  def main
    puts this.add(1,2)
  end
end
#-#-#
MODULE Program 0 0
ENDHDR

PROC Main.add 0 0 0
LDLW 16
LDLW 20
PLUS
RETURNW
END

PROC Main.main 0 0 0
CONST 1
CONST 2
LDLW 12
! method address for Main.add
LDLW 12
LOADW
LDNW 4
CALLW 3
GLOBAL Lib.Print
CALL 1
RETURN
END

PROC Program.%main 4 0 0
CONST 4
GLOBAL Main
LOCAL -4
GLOBAL Lib.New
CALL 3
LDLW -4
GLOBAL Main.main
CALL 1
RETURN
END


DEFINE Object
! Descriptor for Main
DEFINE Main
WORD 0
WORD Main.add
WORD Main.main
DEFINE Main.%super
WORD Object
WORD Main
