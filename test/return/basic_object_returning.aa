comment: testing basic object return code
executable: true
#-#-#
class Wrapper
  int x

  def setX(int y)
    x = y
  end

  def print
    puts x
  end
end

class Printer
  def print(int x) : Wrapper
    Wrapper wrapper
    wrapper = new Wrapper
    wrapper.setX(x)
    wrapper
  end
end

class Main
  def main
    Wrapper wrapper
    Printer printer
    printer = new Printer
    wrapper = printer.print(10)
    wrapper.print
  end
end
