comment: testing that method arguments work as expected
executable: true
#-#-#
class Printer
  def incfive(int x)
    puts x + 5
  end

  def p(int x)
    puts x
  end
end

class Main
  Printer printer

  def main
    printer = new Printer
    printer.incfive(5)
    printer.p(5)
  end
end
#-#-#
MODULE Program 0 0
ENDHDR

PROC Printer.incfive 0 0 0
LDLW 16
CONST 5
PLUS
GLOBAL Lib.Print
CALL 1
RETURN
END

PROC Printer.p 0 0 0
LDLW 16
GLOBAL Lib.Print
CALL 1
RETURN
END

PROC Main.main 0 0 0
CONST 4
GLOBAL Printer
LDLW 12
CONST 4
PLUSA
GLOBAL Lib.New
CALL 3
CONST 5
LDLW 12
LDNW 4
DUP 0
! method address for Printer.incfive
LOADW
LDNW 4
CALL 2
CONST 5
LDLW 12
LDNW 4
DUP 0
! method address for Printer.p
LOADW
LDNW 8
CALL 2
RETURN
END

PROC Program.%main 4 0 0
CONST 8
GLOBAL Main
LOCAL -4
GLOBAL Lib.New
CALL 3
LDLW -4
GLOBAL Main.main
CALL 1
RETURN
END


DEFINE Object
! Descriptor for Printer
DEFINE Printer
WORD 0
WORD Printer.incfive
WORD Printer.p
DEFINE Printer.%super
WORD Printer
WORD Object

! Descriptor for Main
DEFINE Main
WORD 0
WORD Main.main
DEFINE Main.%super
WORD Main
WORD Object
