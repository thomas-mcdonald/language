class Test
  Delegate x
  Delegate y

  def initialize
    x = 0
  end

  def increment
    x = x + 1
  end

  def allow
    int x
    int y
  end
end

class Delegate < Test
  Test counter
  Delegate x

  def increment
    counter.increment
  end
end

class Main
  Test x

  def main
    x = Test.new
    x.increment
  end
end
